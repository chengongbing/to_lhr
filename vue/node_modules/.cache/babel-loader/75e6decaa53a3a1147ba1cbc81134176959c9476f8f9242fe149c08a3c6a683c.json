{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: 'StudentNo',\n  components: {},\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      request.get(\"/student_no\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n    handleYes(row) {\n      // this.form.pass=1\n      console.log(row);\n      row.status = 1;\n      request.put(\"/student_no\", row).then(res => {\n        console.log(res);\n        if (res.code == '0') {\n          this.$message({\n            type: \"success\",\n            message: \"操作成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n        this.dialogVisible = false;\n        this.load();\n      });\n    },\n    handleNo(row) {\n      // this.form.pass=1\n      console.log(row);\n      row.status = -1;\n      request.put(\"/student_no\", row).then(res => {\n        console.log(res);\n        if (res.code == '0') {\n          this.$message({\n            type: \"success\",\n            message: \"操作成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n        this.dialogVisible = false;\n        this.load();\n      });\n    },\n    handleSizeChange(pageSize) {\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.load();\n    },\n    // 新增\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n    }\n    // save() {\n    //   if (this.form.id) {//更新提交\n    //     request.put(\"/user\", this.form).then(res => {\n    //       console.log(res)\n    //       if
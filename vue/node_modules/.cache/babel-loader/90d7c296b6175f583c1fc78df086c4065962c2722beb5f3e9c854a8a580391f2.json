{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: 'Assistance',\n  components: {},\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      request.get(\"/assistance\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search_msg\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n    search() {\n      request.get(\"/assistance\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search_msg: this.search_msg\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n    handleYes(row) {\n      // this.form.pass=1\n      console.log(row);\n      row.status = 1;\n      request.put(\"/assistance\", row).then(res => {\n        console.log(res);\n        if (res.code == '0') {\n          this.$message({\n            type: \"success\",\n            message: \"操作成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n        this.dialogVisible = false;\n        this.load();\n      });\n    },\n    handleNo(row) {\n      // this.form.pass=1\n      console.log(row);\n      row.status = -1;\n      request.put(\"/assistance\", row).then(res => {\n        console.log(res);\n        if (res.code == '0') {\n          this.$message({\n            type: \"success\",\n            message: \"操作成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n        this.dialogVisible = false;\n        this.load();\n      });\n    },\n    handleSizeChange(pageSize) {\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.load();\n    },\n    // 新增\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n    },\n    save() {\n      if (this.form.id) {\n        //更新提交\n        request.put(\"/user\", this.form).then(res => {\n          console.log(res);\n          if (res.code == '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n          this.dialogVisible = false;\n          this.load();\n        });\n      } else {\n        //新增提交\n        request.post(\"/user\", this.form).then(res => {\n          console.log(res);\n          if (res.code == '0') {\n            this.$message({\n              type: \"success\",\n              message: \"新增成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n          this.dialogVisible = false;\n          this.load();\n        });\n      }\n    }\n  },\n  data() {\n    return {\n      search_msg: \"\",\n      total: 0,\n      currentPage: 1,\n      pageSize: 10,\n      pageNum: 1,\n      dialogVisible: false,\n      //弹窗\n      form: {},\n      //新增用户信息\n      tableData: []\n    };\n  }\n};","map":{"version":3,"names":["request","name","components","created","load","methods","get","params","pageNum","currentPage","pageSize","search","search_msg","then","res","console","log","tableData","data","records","total","handleYes","row","status","put","code","$message","type","message","msg","dialogVisible","handleNo","handleSizeChange","handleCurrentChange","add","form","save","id","post"],"sources":["G:\\bs\\to_LHR\\springboot-vue-demo\\vue\\src\\views\\Assistance.vue"],"sourcesContent":["<template>\r\n  <div style=\"padding:10px\">\